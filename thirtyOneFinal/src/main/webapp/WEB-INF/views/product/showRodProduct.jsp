							<img src="/resources/img/productImg/${p.productMainName }"
								class="img-fluid" alt="">
							<c:url var="detailView" value="ProductDetailView.do">
								<c:param name="pNum" value="${p.productNum }" />
							</c:url>
							<div class="portfolio-info">
								<h4>상품명 : ${p.productName }</h4>
								<p>가격 : ${p.productPrice }원</p>
								<div class="portfolio-links">
									<a href="ProductDetailView.do?pNum=${p.productNum }" title="More Details" value ="${p.productNum }"><i
										class="icofont-external-link" ></i></a>
								
								
								</div>
							</div>
						</div>
					</div>
				</c:forEach>
			</div>

